{
  "name": "loki",
  "version": "0.0.1",
  "description": "CLI tool to transform csv files with various delimiter options.",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "format": "prettier --write *.js && standard --fix",
    "test": "ava"
  },
  "keywords": [
    "csv",
    "table",
    "cli"
  ],
  "author": "Fabian GÃ¼ndel",
  "license": "MIT",
  "devDependencies": {
    "ava": "~1.0.0-beta.6",
    "husky": "~1.0.0-rc.13",
    "lint-staged": "~7.2.0",
    "prettier": "~1.14.0",
    "standard": "~11.0.1"
  },
  "dependencies": {
    "arg": "2.0.0",
    "parse-decimal-number": "1.0.0"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "standard --fix",
      "git add"
    ]
  }
}
